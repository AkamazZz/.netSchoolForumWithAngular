import { ElementRef, EventEmitter, OnChanges, OnInit, SimpleChanges } from '@angular/core';
export declare class ToolbarComponent implements OnInit, OnChanges {
    _el: ElementRef;
    itemList: ToolbarItem[];
    color: string;
    direction: 'ltr' | 'rtl';
    actionClick: EventEmitter<ToolbarItem>;
    onKeyup(e: KeyboardEvent): void;
    normalItemList: ToolbarItem[];
    floatItemList: ToolbarItem[];
    constructor(_el: ElementRef);
    ngOnChanges(changes: SimpleChanges): void;
    ngOnInit(): void;
    item_onClick(item: ToolbarItem): void;
}
export interface ToolbarItem {
    id: number;
    name?: string;
    tooltip?: string;
    matIcon?: string;
    matIconColor?: string;
    svgIcon?: string;
    splitter?: boolean;
    disable?: boolean;
    float?: boolean;
}
